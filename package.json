{
  "name": "truffle-hardhat",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "hardhat node",
    "test": "hardhat test",
    "precompile": "rm -r $(pwd)/client/src/utils/abi && mkdir $(pwd)/client/src/utils/abi",
    "compile": "hardhat compile",
    "postcompile": "mv $(pwd)/artifacts/contracts/*/*.json $(pwd)/client/src/utils/abi",
    "testnet:deploy": "hardhat deploy --tags NFT1155 --network testnet",
    "mainnet:deploy": "hardhat deploy --tags NFT1155 --network mainnet",
    "t:verify": "hardhat --network testnet etherscan-verify --sleep",
    "m:verify": "hardhat --network mainnet etherscan-verify --sleep"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.1",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "ethers": "^5.0.32",
    "hardhat": "^2.0.10",
    "hardhat-gas-reporter": "^1.0.4",
    "solidity-coverage": "^0.7.20",
    "web3": "^1.3.3"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.8.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "chai": "^4.3.4",
    "dotenv": "^8.2.0",
    "hardhat-deploy": "^0.11.23",
    "ipfs-http-client": "^49.0.4"
  }
}
